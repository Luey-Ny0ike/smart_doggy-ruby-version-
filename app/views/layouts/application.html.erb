<!DOCTYPE html>
<html>
  <head>
    <title>SmartDoggy</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
  </head>



  <body>
    <% unless flash.empty? %>
         <script type="text/javascript">
              <% flash.each do |f| %>
            <% type = f[0].to_s.gsub('alert', 'error').gsub('notice', 'info') %>
             toastr['<%= type %>']('<%= f[1] %>');
         <% end %>
         </script>
    <% end %>

<div class="sidebar-nav">
  <ul id="slide-out" class="side-nav">
    <li><div class="user-view">
      <h4>Smart Doggy</h4>
    </div></li>
    <li><a href="/">Home</a></li>
    <li><a href="/tricks">Contact us</a></li>
    <li><a href="/tricks">About us</a></li>
    <li><div class="divider"></div></li>
    <li><a class="subheader">Services</a></li>
    <li><a class="waves-effect" href="/tricks">Train your puppy!</a></li>
    <li><a class="waves-effect" href="/testimonials">View testimonials</a></li>
  </ul>
  <div class="panel panel-default">
    <div class="panel-heading"><a href="#" data-activates="slide-out" class="button-collapse"><i class="material-icons">menu</i></a></div>
  </div>
  </div>
  <script type="text/javascript">
    $(".button-collapse").sideNav();
  </script>
</div>
</div>

  </body>
</html>
